pipeline {
    agent any
    tools {
  maven 'M2_HOME'
}

    stages {
        stage('build') {
            steps {
                echo 'mvn clean'
                echo 'mvn install'
                echo 'mvn package'
            }
        }
        stage('test') {
            steps {
                echo 'mvn test'
            }
        }
        stage('deploy') {
            steps {
                echo 'mvn deploy'
            }
        }
       
       
    }
}

